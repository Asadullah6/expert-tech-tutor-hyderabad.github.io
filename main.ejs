    <!-- Swiper CSS (for testimonial slider) -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/Swiper/8.4.7/swiper-bundle.min.css" integrity="sha512-f7Ap8oREL5d/0N+TuD47nGhtb7SK6kNbOxE+6Wf4gOWw+/s9Y8HXF42vJBIBQb5WwuMp7RHGx0/cDk5j5Y7nrg==" crossorigin="anonymous" referrerpolicy="no-referrer">
    
    <!-- Custom CSS -->
    <link rel="stylesheet" href="/css/style.css">
    
    <!-- Additional Page-Specific CSS -->
    <% if (typeof additionalCSS !== 'undefined') { %>
        <% additionalCSS.forEach(function(cssFile) { %>
            <link rel="stylesheet" href="<%= cssFile %>">
        <% }); %>
    <% } %>
    
    <!-- Structured Data (JSON-LD) -->
    <script type="application/ld+json">
    {
        "@context": "https://schema.org",
        "@type": "EducationalOrganization",
        "name": "Expert Tutors Hyderabad",
        "description": "Premier technology tutoring services in Hyderabad offering programming, AI, data science, and web development courses",
        "url": "https://experttutorshyderabad.com",
        "telephone": "+91-9876543210",
        "address": {
            "@type": "PostalAddress",
            "streetAddress": "Hitech City",
            "addressLocality": "Hyderabad",
            "addressRegion": "Telangana",
            "postalCode": "500081",
            "addressCountry": "IN"
        },
        "areaServed": [
            "Hitech City", "Gachibowli", "Madhapur", "Banjara Hills", 
            "Jubilee Hills", "Secunderabad", "Begumpet", "Ameerpet"
        ],
        "serviceType": [
            "Web Development Tutoring",
            "Python Programming",
            "AI and Machine Learning",
            "Data Science Training",
            "Mobile App Development",
            "Software Engineering"
        ],
        "aggregateRating": {
            "@type": "AggregateRating",
            "ratingValue": "4.7",
            "reviewCount": "4523"
        }
    }
    </script>
    
